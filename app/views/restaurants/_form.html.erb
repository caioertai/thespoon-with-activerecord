<%= form_for @restaurant do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :address %>
  <%= form.text_field :address %>
  <br>
  <%= form.label :stars %>
  <%= form.text_field :stars %>
  <br>
  <%= form.submit %>
<% end %>

